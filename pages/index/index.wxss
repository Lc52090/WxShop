/* 轮播图开始 */
.index-swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index-swiper swiper navigator image {
  width: 100%;
}
/* 导航开始 */
.idnex-cate {
  display: flex;
  margin-bottom: 20rpx;
}
.idnex-cate navigator {
  flex: 1;
  height: 210rpx;
}
.idnex-cate navigator image {
  width: 100%;
  height: 100%;
}
.index-floor .floor-group {
  padding: 10rpx 0;
  /* 标题 */
  /* 内容 */
}
.index-floor .floor-group .floor-title image {
  width: 100%;
}
.index-floor .floor-group .floor-list {
  overflow: hidden;
}
.index-floor .floor-group .floor-list navigator {
  float: left;
  width: 33.33%;
  /* 后四张图片 */
}
.index-floor .floor-group .floor-list navigator:nth-last-child(-n+4) image {
  height: 27.72711207vw;
  border-left: 10rpx solid #ffffff;
}
.index-floor .floor-group .floor-list navigator:nth-child(2) image,
.index-floor .floor-group .floor-list navigator:nth-child(3) image {
  border-bottom: 10rpx solid #ffffff;
}
.index-floor .floor-group .floor-list navigator image {
  width: 100%;
  height: 100%;
  display: block;
}
